SELECT
    LOCATION,
    COUNT(*)
FROM _GLOBAL_WOLVES GROUP BY LOCATION ORDER BY COUNT(*) DESC LIMIT 5;


SELECT
    LOCATION,
    COUNT(*)
FROM _GLOBAL_WOLVES GROUP BY LOCATION ORDER BY COUNT(*) DESC LIMIT 5;


SELECT
    LOCATION,
    COUNT(*) AS NUM_ATTACKS
FROM _GLOBAL_WOLVES GROUP BY LOCATION ORDER BY NUM_ATTACKS DESC LIMIT 5;
